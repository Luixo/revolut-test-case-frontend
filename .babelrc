{
  "plugins": [
    "transform-class-properties",
    "syntax-dynamic-import",
    "transform-async-to-generator",
    [
      "transform-runtime",
      {
        "helpers": true,
        "polyfill": false,
        "regenerator": true
      }
    ],
    [
      "transform-object-rest-spread",
      {
        "useBuiltIns": true
      }
    ]
  ],
  "presets": [
    "react",
    "es2015",
    ["env", {
      "modules": false,
      "targets": {
        "ie9": true
      },
      "uglify": true
    }]
  ]
}